{
  "name": "badeblog-frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --build --force && vite build",
    "preview": "vite preview --port ${PORT:-4173}",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "start": "vite preview --port ${PORT:-4173} --host",
    "test": "jest --config jest.config.mjs --transformIgnorePatterns \"node_modules/(?!@vue|vue-router)/\"",
    "test:watch": "jest --config jest.config.mjs --watch",
    "test:coverage": "jest --config jest.config.mjs --coverage"
  },
  "dependencies": {
    "@types/quill": "^2.0.14",
    "@vueup/vue-quill": "^1.2.0",
    "axios": "^1.7.7",
    "pinia": "^2.2.6",
    "quill": "^2.0.2",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-typescript": "7.26.0",
    "@tailwindcss/typography": "^0.5.15",
    "@tsconfig/node22": "^22.0.0",
    "@types/jest": "29.5.14",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/test-utils": "2.4.0-alpha.2",
    "@vue/tsconfig": "^0.5.1",
    "@vue/vue3-jest": "29.2.6",
    "autoprefixer": "^10.4.20",
    "babel-jest": "29.7.0",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "npm-run-all2": "^7.0.1",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.14",
    "ts-jest": "29.2.5",
    "typescript": "~5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-vue-devtools": "^7.5.4",
    "vitest": "2.1.5",
    "vue-tsc": "^2.1.10"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
